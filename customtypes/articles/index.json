{
  "id": "articles",
  "label": "Articles",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "slug",
          "placeholder": ""
        }
      },
      "articletitle": {
        "type": "StructuredText",
        "config": {
          "label": "articleTitle",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "articletype": {
        "type": "Select",
        "config": {
          "label": "articleType",
          "placeholder": "Select Article Type",
          "options": [
            "text",
            "video",
            "podcast",
            "review"
          ],
          "default_value": "text"
        }
      },
      "author": {
        "type": "StructuredText",
        "config": {
          "label": "Author",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "publish_date": {
        "type": "Date",
        "config": {
          "label": "Publish Date",
          "placeholder": ""
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "",
          "options": [
            "1",
            "2"
          ]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "article_lead_paragraph": {
              "type": "SharedSlice"
            },
            "video": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "Meta": {
      "meta_title": {
        "type": "StructuredText",
        "config": {
          "label": "Meta Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "meta_description": {
        "type": "StructuredText",
        "config": {
          "label": "Meta Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "meta_url": {
        "type": "StructuredText",
        "config": {
          "label": "Meta URL",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "MEta Image",
          "constraint": {},
          "thumbnails": []
        }
      }
    }
  }
}